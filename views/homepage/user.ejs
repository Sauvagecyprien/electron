
<div class="container  p-5">


    <h1 class="text-center">Affichage des Individus :</h1>
    <br/>
    <div class="card">

        <div class="card-body">
            <ul class="nav justify-content-center">



                <li class="nav-item">
                    <a class="nav-link" href="#">resultat du echo des donn√©e user</a>
                </li>

            </ul>
            <table class="table table-striped table-dark">
                <thead>
                <tr>
                    <th scope="col">#id</th>
                    <th scope="col">nom</th>
                    <th scope="col">prenom</th>
                    <th scope="col">entreprise</th>
                    <th scope="col">email</th>
                </tr>
                </thead>
                <tbody>

                <% for(var i=0; i<test.length; i++) {%>
                    <tr>
                        <th scope="row"><%= test[i].id %></th>
                        <td><%= test[i].nom %></td>
                        <td><%= test[i].prenom %></td>
                        <td><%= test[i].entreprise %></td>
                        <td><%= test[i].email %></td>
                        <td><form action="/deleteuser/<%= test[i].id %>" method="post"><button type="submit" class="btn btn-dark btn-block">supprimer</button></form></td>
                    </tr>
                <% } %>


                </tbody>
            </table>

        </div>


    </div>

</div>
