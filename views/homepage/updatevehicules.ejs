<div class="container  p-5">

    <h1 class="text-center">Modification d'une voiture </h1>
    <br/>
    <div class="card">

        <div class="card-body">
            <form action="/upvehicules" method="post">
                <div class="form-group">
                    <label for="exampleInputEmail1">Marque : </label>
                    <input type="hidden" name="id" value="<%= vehicules.id %>">
                    <input type="text" class="form-control" name="marques" id="exampleInputEmail1" value="<%= vehicules.marques %>">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail1">Modele</label>
                    <input type="text" class="form-control" name="modele" id="exampleInputEmail1" value="<%= vehicules.modele %>">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail1">Puissance</label>
                    <input type="text" class="form-control" name="puissance" id="exampleInputEmail1" value="<%= vehicules.puissance %>">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail1">Ann√©e</label>
                    <input type="date" class="form-control" name="annee" id="" value="<%= vehicules.annee %>">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Immatriculations:</label>
                    <input type="text" class="form-control" name="immatriculations" value="<%= vehicules.immatriculations %>">
                </div>

                <button type="submit" id="buttons" class="btn btn-dark btn-block">Inscription</button>
            </form>
        </div>


    </div>

</div>

